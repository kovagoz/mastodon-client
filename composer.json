{
  "name": "kovagoz/prototype",
  "description": "Minimal HTTP stack for PHP projects",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Zoltan Kovago",
      "email": "kovago@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0.1",
    "illuminate/container": "^9.22",
    "nyholm/psr7": "^1.5",
    "nyholm/psr7-server": "^1.0",
    "aura/router": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "filp/whoops": "^2.14.5",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
